/******************************************************************************

time compexity is o(1)

*******************************************************************************/

#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int fact(int n){
    if(n==0 || n==1){
        return 1;
    }
    else{
        return fact(n-1)*n;
}
    
    
    
    
    
    }



int main()
{
   
    cout<<fact(6);
    

    return 0;
}
