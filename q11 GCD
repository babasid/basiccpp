/******************************************************************************
TIME COMPEXITY O[1]
SPACE COMPLEXITY O[1]

*******************************************************************************/

#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int gcd(int a,int b){
    if(a==0){
        return b;
    }
    if(b==0){
        return a;
    }
    else{
        return gcd(b%a,a);
    }
    
    
    
    
    
    }



int main()
{
   
    cout<<gcd(15,7);
    

    return 0;
}
