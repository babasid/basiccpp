/******************************************************************************

   time complexity O(SQRT(N))

*******************************************************************************/

#include <iostream>
#include<bits/stdc++.h>

using namespace std;
int squareroot(int n){
    int i;
    for(i=0;i<n;i++){
        
        if(i*i==n){
            return i;
        }
        if(i*i>n){
            return i-1;
        }
    }
}


int main()
{
    cout<<squareroot(27);

    return 0;
}
